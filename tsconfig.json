{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "#types/*":[
        "types/*"
      ],
      "$screens/*":[
        "screens/*"
      ],
      "?hooks/*": [
        "hooks/*"
      ]
    }
  }
}
